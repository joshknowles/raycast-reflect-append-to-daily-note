{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "reflect-append-to-daily-note",
  "title": "reflect-append-to-daily-note",
  "description": "",
  "icon": "command-icon.png",
  "author": "joshknowles",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "authorizationToken",
      "title": "Authorization Token",
      "description": "Reflect Authorization Token",
      "type": "password",
      "required": true
    }
  ],
  "commands": [
    {
      "name": "reflect",
      "title": "Append to Daily Note",
      "subtitle": "Reflect",
      "description": "Append to the daily note within the Reflect app.",
      "keywords": [
        "daily note",
        "note today",
        "daily"
      ],
      "preferences": [
        {
          "name": "graphId",
          "title": "Graph ID",
          "description": "Reflect Graph ID",
          "type": "textfield",
          "required": true
        },
        {
          "name": "listName",
          "title": "List Name",
          "description": "Name of the list to append to",
          "type": "textfield",
          "required": false
        }
      ],
      "arguments": [
        {
          "name": "text",
          "type": "text",
          "placeholder": "Note",
          "required": true
        }
      ],
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.2",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
